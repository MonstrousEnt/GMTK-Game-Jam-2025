@export var items = []
var current_item: Area2D = null

func pickup(item: Area2D):
	current_item = item
	items.append(item)
	print(\"Item added to inventory. Items array now holds \", items.size(), \" items.\")
	item.queue_free()

func use_item():
	items[0].use()
"

